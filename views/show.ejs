    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%=listing.title  %></title>
</head>
<body>
    <ul>
      <%=listing.title  %>
        <li><%=listing.description  %></li>
        <li><%=//listing.image  %></li>
